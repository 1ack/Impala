From 46c0d5460b742dda064a7e2d7e3247559cd93e3e Mon Sep 17 00:00:00 2001
From: Zhenxiao Luo <zhenxiao@cloudera.com>
Date: Tue, 3 Jul 2012 12:34:04 -0700
Subject: [PATCH 047/144] CLOUDERA-BUILD. Restricting the included shims to 0.23 only

---
 build.properties |    1 +
 shims/build.xml  |    4 ----
 2 files changed, 1 insertions(+), 4 deletions(-)

diff --git a/src/build.properties b/src/build.properties
index 7c342e2..ec3acbc 100644
--- a/src/build.properties
+++ b/src/build.properties
@@ -30,6 +30,7 @@ hadoop-0.20.version=0.20.2
 hadoop-0.20S.version=1.0.0
 hadoop-0.23.version=0.23.1
 hadoop.version=${hadoop-0.23.version}
+shims.include=0.23
 hadoop.security.version=${hadoop-0.23.version}
 hadoop.mirror=http://mirror.facebook.net/facebook/hive-deps
 hadoop.mirror2=http://archive.cloudera.com/hive-deps
diff --git a/src/shims/build.xml b/src/shims/build.xml
index 906abed..f9182f4 100644
--- a/src/shims/build.xml
+++ b/src/shims/build.xml
@@ -133,9 +133,5 @@ to call at top-level: ant deploy-contrib compile-core-test
   
   <target name="compile-test" depends="compile,ivy-retrieve-test">
     <echo message="Project: ${ant.project.name}"/>
-    <!-- TODO: move tests to version directory -->
-    <antcall target="compile_secure_test" inheritRefs="false" inheritAll="false">
-      <param name="hadoop.version.ant-internal" value="${hadoop.security.version}" />
-    </antcall>
   </target>
 </project>
